<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key><string>com.musai.lms.autoload</string>

  <key>ProgramArguments</key>
  <array>
    <string>/bin/bash</string>
    <string>-lc</string>
    <string>~/.lmstudio/bin/lms load mem-agent-mlx || ~/.lmstudio/bin/lms load driaforall/mem-agent-mlx-4bit</string>
  </array>

  <key>EnvironmentVariables</key>
  <dict>
    <key>PATH</key>
    <string>/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/Users/${USER}/.local/bin:/Users/${USER}/.lmstudio/bin</string>
  </dict>

  <key>RunAtLoad</key><true/>
  <key>KeepAlive</key><true/>

  <key>StandardOutPath</key><string>/Users/${USER}/Library/Logs/lms-autoload.out</string>
  <key>StandardErrorPath</key><string>/Users/${USER}/Library/Logs/lms-autoload.err</string>
</dict>
</plist>
